---
import logo50 from "../../../images/logo50.png"

interface Props {
    idSuffix?: string;
}
const { idSuffix = "-menu"} = Astro.props;

const URL = import.meta.env.BASE_URL;
---
<a
    id={`logo-button${idSuffix}`} 
    href={`${URL}`}
    class="nav-button"
>
    <span class="logo-text">Health Analytics MX</span>
    <!-- <img src={logo50} alt="Health Analytics MX Logo" height="40px" width="40px"/> -->
</a>